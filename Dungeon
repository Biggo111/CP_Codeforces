#include <bits/stdc++.h>
using namespace std;
#define ll long long int

int main() {
    
    ll t;
    cin>>t;
    while(t--)
    {
        ll a,b,c;
        cin>>a>>b>>c;
        ll s=a+b+c;
        if(s%9==0)
        {
            ll mn=min(a,min(b,c));
            if(mn>=(s/9))
            {
                cout<<"YES"<<endl;
            }
            else cout<<"NO"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
}
