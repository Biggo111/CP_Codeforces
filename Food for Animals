#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define MOD 1000000007
typedef unsigned long long int ull;
 
int main() {
    ll t;
    cin>>t;
    while(t--)
    {
        ll a,b,c,x,y;
        cin>>a>>b>>c>>x>>y;
        x = max(0*1LL,x-a);
        y = max(0*1LL,y-b);
        if(c>=x+y)
        {
            cout<<"YES"<<endl;
        }
        else cout<<"NO"<<endl;
    }
}
