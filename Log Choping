#include <bits/stdc++.h>
using namespace std;
#define ll long long int
 
int main() {
    
    ll t;
    cin>>t;
    while(t--)
    {
        ll n;
        cin>>n;
        vector<ll>v(n);
        ll k=0;
        for(int i=0;i<n;i++)
        {
            cin>>v[i];
        }
        for(int i=0;i<n;i++)
        {
            k+=(v[i]-1);
        }
        if(k%2!=0)
        {
            cout<<"errorgorn"<<endl;
        }
        else cout<<"maomao90"<<endl;
    }
}
