#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define MOD 1000000007
typedef unsigned long long int ull;
 
int main() {
    ll t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        ll s1 = (s[0]-'0')+(s[1]-'0')+(s[2]-'0');
        ll s2 = (s[3]-'0')+(s[4]-'0')+(s[5]-'0');
        if(s1==s2)
        {
            cout<<"YES"<<endl;
        }
        else cout<<"NO"<<endl;
    }
}
